package practice;

public class Reverse {
  public static void main(String[] args) {
	 Reverse ref=new Reverse();
	 int start=100;
	 int end=300;
	 for(int a=start;a<=end;a++)
	 {
		 System.out.println(ref.revNumber(a));
	 }
}
  public int revNumber (int a)
  {
	  int rev=0;
	  while(a>0)
	  {
		  int rem=a%10;
		  rev=rev*10+rem;
		  a=a/10;
	  }
	  return rev;
  }
}
